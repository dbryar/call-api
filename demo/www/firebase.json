{
  "hosting": {
    "site": "opencall-www",
    "public": ".",
    "ignore": ["firebase.json", "**/node_modules/**", "**/src/**", "Dockerfile", "package.json", "bun.lock*", "tsconfig.json"],
    "headers": [
      {
        "source": "**",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=3600" }
        ]
      }
    ]
  }
}
